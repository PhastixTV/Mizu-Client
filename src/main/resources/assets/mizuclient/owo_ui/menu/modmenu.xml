<owo-ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/wisp-forest/owo-lib/1.20.3/owo-ui.xsd">
    <components>
        <flow-layout direction="vertical">
            <children>
                <flow-layout direction="vertical">
                    <children>



                        <flow-layout direction="vertical" id="modmenu-box-container">
                            <children>
                                <box>
                                    <color>black</color>
                                    <sizing>
                                        <horizontal method="fixed">10</horizontal>
                                        <vertical method="fixed">10</vertical>
                                    </sizing>

                                </box>
                                <button id = "test">
                                    <text>ENABLED</text>
                                </button>
                            </children>
                            <surface>
                                <flat>#77000000</flat>
                                <outline>#FF121212</outline>
                            </surface>

                            <padding>
                                <all>5</all>
                            </padding>

                            <sizing>
                                <vertical method="fixed">250</vertical>
                                <horizontal method="fixed">250</horizontal>
                            </sizing>

                            <horizontal-alignment>center</horizontal-alignment>
                            <vertical-alignment>center</vertical-alignment>
                        </flow-layout>
                    </children>

                    <vertical-alignment>center</vertical-alignment>
                    <horizontal-alignment>center</horizontal-alignment>
                    <surface>
                        <vanilla-translucent/>
                    </surface>
                </flow-layout>
            </children>
        </flow-layout>
    </components>
</owo-ui>
